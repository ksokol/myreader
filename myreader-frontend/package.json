{
  "name": "myreader-frontend",
  "description": "Myreader frontend package",
  "scripts": {
    "build": "export NODE_ENV=production && webpack --bail --progress --profile",
    "test": "export NODE_ENV=test && jest --coverage --ci --silent",
    "start": "export NODE_ENV=development && webpack serve  --history-api-fallback --progress",
    "report": "export NODE_ENV=production && webpack --bail --progress --profile && webpack-bundle-analyzer dist/stats.json",
    "lint": "npm run lint:js-report && npm run lint:lockfile",
    "lint:js-report": "eslint -f json -o build/reports/eslint.json src/app/js",
    "lint:js": "eslint src/app/js",
    "lint:lockfile": "npx lockfile-lint -p package-lock.json -s -a npm -t npm",
    "audit:high": "npm audit --audit-level=high"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "5.15.4",
    "@jaames/iro": "5.5.2",
    "dialog-polyfill": "0.5.6",
    "prop-types": "15.7.2",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-hotkeys-hook": "3.4.4",
    "react-intersection-observer": "8.32.5",
    "react-router": "5.2.0",
    "react-router-dom": "5.2.0"
  },
  "devDependencies": {
    "@babel/core": "7.16.0",
    "@babel/eslint-parser": "7.16.3",
    "@babel/preset-env": "7.16.4",
    "@babel/preset-react": "7.16.0",
    "@testing-library/jest-dom": "5.16.0",
    "@testing-library/react": "12.1.2",
    "babel-jest": "27.4.2",
    "babel-loader": "8.2.3",
    "babel-plugin-transform-react-remove-prop-types": "0.4.24",
    "css-loader": "6.5.1",
    "css-minimizer-webpack-plugin": "3.2.0",
    "eslint": "8.4.0",
    "eslint-plugin-jest": "25.3.0",
    "eslint-plugin-jest-dom": "3.9.2",
    "eslint-plugin-react": "7.27.1",
    "eslint-plugin-react-hooks": "4.3.0",
    "eslint-plugin-sonarjs": "0.11.0",
    "eslint-plugin-testing-library": "5.0.1",
    "eslint-plugin-unicorn": "39.0.0",
    "html-webpack-plugin": "5.5.0",
    "jest": "27.4.3",
    "jest-sonar-reporter": "2.0.0",
    "lockfile-lint": "4.6.2",
    "lodash.isequal": "4.5.0",
    "mini-css-extract-plugin": "2.4.5",
    "raw-loader": "4.0.2",
    "register-service-worker": "1.7.2",
    "terser-webpack-plugin": "5.2.5",
    "webpack": "5.64.4",
    "webpack-bundle-analyzer": "4.5.0",
    "webpack-cli": "4.9.1",
    "webpack-dev-server": "4.6.0",
    "webpack-pwa-manifest": "4.3.0",
    "workbox-webpack-plugin": "6.4.2"
  },
  "directories": {
    "test": "test"
  },
  "jestSonar": {
    "reportPath": "build",
    "reportFile": "TESTS.xml"
  }
}

{
  "name": "myreader-frontend",
  "description": "Myreader frontend package",
  "scripts": {
    "snyk": "snyk test",
    "build": "export NODE_ENV=production && webpack --bail --progress --profile --display-optimization-bailout",
    "server": "export NODE_ENV=development && webpack-dev-server --history-api-fallback --inline --progress",
    "test": "export NODE_ENV=test && jest --coverage --ci --silent",
    "test-watch": "export NODE_ENV=test && jest --watch --verbose false",
    "start": "npm run server",
    "report": "export NODE_ENV=production && webpack --bail --progress --profile && webpack-bundle-analyzer dist/stats.json",
    "lint": "npm run lint:js && npm run lint:css",
    "lint:js": "eslint -c .eslintrc.js src/app/js",
    "lint:css": "stylelint \"src/app/js/**/*.css\" && stylelint \"src/app/css/**/*.css\""
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "1.2.7",
    "@fortawesome/free-solid-svg-icons": "5.4.2",
    "@fortawesome/react-fontawesome": "0.1.3",
    "@jaames/iro": "3.5.4",
    "@uirouter/core": "5.0.21",
    "@uirouter/react": "0.8.7",
    "classnames": "2.2.6",
    "dialog-polyfill": "0.4.10",
    "downshift": "3.1.7",
    "lodash.debounce": "4.0.8",
    "lodash.isequal": "4.5.0",
    "prop-types": "15.6.2",
    "react": "16.6.0",
    "react-dom": "16.6.0",
    "react-redux": "5.1.0",
    "redux": "4.0.1",
    "redux-thunk": "2.3.0",
    "reselect": "4.0.0",
    "timeago.js": "3.0.2"
  },
  "devDependencies": {
    "@babel/core": "7.2.2",
    "@babel/plugin-proposal-object-rest-spread": "7.0.0",
    "@babel/preset-env": "7.1.0",
    "@babel/preset-react": "7.0.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "23.6.0",
    "babel-loader": "8.0.5",
    "clean-webpack-plugin": "1.0.0",
    "css-loader": "2.1.0",
    "cssnano": "4.1.8",
    "enzyme": "3.8.0",
    "enzyme-adapter-react-16": "1.7.1",
    "eslint": "5.12.1",
    "eslint-plugin-jest": "22.1.3",
    "eslint-plugin-react": "7.12.4",
    "file-loader": "3.0.1",
    "html-loader-jest": "0.2.1",
    "html-webpack-plugin": "3.2.0",
    "intersection-observer": "0.5.1",
    "jest": "23.6.0",
    "jest-css-modules-transform": "2.1.1",
    "jest-fetch-mock": "1.6.6",
    "jest-sonar-reporter": "2.0.0",
    "lodash.ismatch": "4.4.0",
    "mini-css-extract-plugin": "0.5.0",
    "mock-local-storage": "1.1.8",
    "null-loader": "0.1.1",
    "optimize-css-assets-webpack-plugin": "5.0.1",
    "raw-loader": "1.0.0",
    "redux-mock-store": "1.5.3",
    "snyk": "1.105.0",
    "stylelint": "9.10.1",
    "stylelint-config-standard": "18.2.0",
    "svg-url-loader": "2.3.2",
    "sw-precache-webpack-plugin": "0.11.5",
    "webpack": "4.23.1",
    "webpack-bundle-analyzer": "3.0.3",
    "webpack-cli": "3.1.2",
    "webpack-dev-server": "3.1.10",
    "webpack-pwa-manifest": "3.7.1"
  },
  "directories": {
    "test": "test"
  },
  "jestSonar": {
    "reportPath": "build",
    "reportFile": "TESTS.xml"
  }
}

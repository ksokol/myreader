{
  "name": "myreader-frontend",
  "description": "Myreader frontend package",
  "scripts": {
    "preinstall": "npx npm-force-resolutions",
    "build": "export NODE_ENV=production && webpack --bail --progress --profile --display-optimization-bailout",
    "server": "export NODE_ENV=development && webpack-dev-server --history-api-fallback --inline --progress",
    "test": "export NODE_ENV=test && jest --coverage --ci --silent",
    "test-watch": "export NODE_ENV=test && jest --watch --verbose false",
    "start": "npm run server",
    "report": "export NODE_ENV=production && webpack --bail --progress --profile && webpack-bundle-analyzer dist/stats.json",
    "lint": "npm run lint:js-report && npm run lint:lockfile",
    "lint:js-report": "eslint -f json -o build/reports/eslint.json src/app/js",
    "lint:js": "eslint src/app/js",
    "lint:lockfile": "npx lockfile-lint -p package-lock.json -s -a npm -t npm",
    "audit:high": "npm audit --audit-level=high"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "5.12.1",
    "@jaames/iro": "5.1.5",
    "classnames": "2.2.6",
    "dialog-polyfill": "0.5.0",
    "lodash.debounce": "4.0.8",
    "prop-types": "15.7.2",
    "react": "16.13.0",
    "react-dom": "16.13.0",
    "react-router": "5.1.2",
    "react-router-dom": "5.1.2"
  },
  "devDependencies": {
    "@babel/core": "7.11.0",
    "@babel/plugin-proposal-class-properties": "7.10.4",
    "@babel/preset-env": "7.11.0",
    "@babel/preset-react": "7.10.4",
    "babel-eslint": "10.1.0",
    "babel-jest": "26.2.2",
    "babel-loader": "8.1.0",
    "babel-plugin-transform-react-remove-prop-types": "0.4.24",
    "clean-webpack-plugin": "3.0.0",
    "css-loader": "4.2.0",
    "cssnano": "4.1.10",
    "enzyme": "3.11.0",
    "enzyme-adapter-react-16": "1.15.2",
    "eslint": "7.6.0",
    "eslint-plugin-jest": "23.20.0",
    "eslint-plugin-react": "7.20.5",
    "eslint-plugin-react-hooks": "4.0.8",
    "eslint-plugin-unicorn": "21.0.0",
    "file-loader": "6.0.0",
    "html-webpack-plugin": "4.3.0",
    "jest": "26.2.2",
    "jest-sonar-reporter": "2.0.0",
    "lockfile-lint": "4.3.7",
    "lodash.ismatch": "4.4.0",
    "mini-css-extract-plugin": "0.9.0",
    "optimize-css-assets-webpack-plugin": "5.0.3",
    "raw-loader": "4.0.1",
    "svg-url-loader": "6.0.0",
    "sw-precache-webpack-plugin": "1.0.0",
    "terser-webpack-plugin": "3.0.8",
    "webpack": "4.44.1",
    "webpack-bundle-analyzer": "3.8.0",
    "webpack-cli": "3.3.12",
    "webpack-dev-server": "3.11.0",
    "webpack-pwa-manifest": "4.2.0"
  },
  "resolutions": {
    "minimist": "1.2.5",
    "dot-prop": "5.2.0"
  },
  "directories": {
    "test": "test"
  },
  "jestSonar": {
    "reportPath": "build",
    "reportFile": "TESTS.xml"
  }
}

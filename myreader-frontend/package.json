{
  "name": "myreader-frontend",
  "description": "Myreader frontend package",
  "scripts": {
    "build": "export NODE_ENV=production && webpack --bail --progress --profile",
    "server": "export NODE_ENV=development && webpack serve  --history-api-fallback --inline --progress",
    "test": "export NODE_ENV=test && jest --coverage --ci --silent",
    "test-watch": "export NODE_ENV=test && jest --watch --verbose false",
    "start": "npm run server",
    "report": "export NODE_ENV=production && webpack --bail --progress --profile && webpack-bundle-analyzer dist/stats.json",
    "lint": "npm run lint:js-report && npm run lint:lockfile",
    "lint:js-report": "eslint -f json -o build/reports/eslint.json src/app/js",
    "lint:js": "eslint src/app/js",
    "lint:lockfile": "npx lockfile-lint -p package-lock.json -s -a npm -t npm",
    "audit:high": "npm audit --audit-level=high"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "5.15.3",
    "@jaames/iro": "5.3.2",
    "classnames": "2.2.6",
    "dialog-polyfill": "0.5.6",
    "prop-types": "15.7.2",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-hotkeys-hook": "3.2.1",
    "react-router": "5.2.0",
    "react-router-dom": "5.2.0"
  },
  "devDependencies": {
    "@babel/core": "7.13.10",
    "@babel/plugin-proposal-class-properties": "7.13.0",
    "@babel/preset-env": "7.13.10",
    "@babel/preset-react": "7.12.13",
    "@testing-library/jest-dom": "5.11.9",
    "@testing-library/react": "11.2.5",
    "babel-eslint": "10.1.0",
    "babel-jest": "26.6.3",
    "babel-loader": "8.2.2",
    "babel-plugin-transform-react-remove-prop-types": "0.4.24",
    "css-loader": "5.1.3",
    "cssnano": "4.1.10",
    "enzyme": "3.11.0",
    "enzyme-adapter-react-16": "1.15.6",
    "eslint": "7.12.1",
    "eslint-plugin-jest": "24.1.0",
    "eslint-plugin-jest-dom": "3.2.4",
    "eslint-plugin-react": "7.21.5",
    "eslint-plugin-react-hooks": "4.2.0",
    "eslint-plugin-testing-library": "3.9.2",
    "eslint-plugin-unicorn": "23.0.0",
    "html-webpack-plugin": "5.3.1",
    "jest": "26.6.3",
    "jest-sonar-reporter": "2.0.0",
    "lockfile-lint": "4.3.7",
    "lodash.isequal": "4.5.0",
    "lodash.ismatch": "4.4.0",
    "mini-css-extract-plugin": "1.3.9",
    "optimize-css-assets-webpack-plugin": "5.0.4",
    "raw-loader": "4.0.2",
    "register-service-worker": "1.7.2",
    "svg-url-loader": "7.1.1",
    "terser-webpack-plugin": "5.1.1",
    "webpack": "5.26.3",
    "webpack-bundle-analyzer": "4.4.0",
    "webpack-cli": "4.5.0",
    "webpack-dev-server": "3.11.2",
    "webpack-pwa-manifest": "4.3.0",
    "workbox-webpack-plugin": "6.1.2"
  },
  "directories": {
    "test": "test"
  },
  "jestSonar": {
    "reportPath": "build",
    "reportFile": "TESTS.xml"
  }
}

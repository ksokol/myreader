{
  "name": "myreader-frontend",
  "description": "Myreader frontend package",
  "scripts": {
    "snyk": "snyk test",
    "build": "export NODE_ENV=production && webpack --bail --progress --profile --display-optimization-bailout",
    "server": "export NODE_ENV=development && webpack-dev-server --history-api-fallback --inline --progress",
    "test": "export NODE_ENV=test && jest --coverage --ci --silent",
    "test-watch": "export NODE_ENV=test && jest --watch",
    "start": "npm run server",
    "report": "export NODE_ENV=production && webpack --bail --progress --profile && webpack-bundle-analyzer dist/stats.json",
    "lint": "npm run lint:js && npm run lint:css",
    "lint:js": "eslint -c .eslintrc.js src/app/js",
    "lint:css": "stylelint \"src/app/js/**/*.css\" && stylelint \"src/app/css/**/*.css\""
  },
  "dependencies": {
    "@uirouter/angularjs": "1.0.14",
    "angular": "1.6.9",
    "classnames": "2.2.5",
    "lodash.debounce": "4.0.8",
    "material-design-icons": "3.0.1",
    "ng-redux": "3.5.2",
    "ngreact": "0.5.1",
    "prop-types": "15.6.1",
    "react": "16.4.1",
    "react-dom": "16.4.1",
    "redux": "4.0.0",
    "redux-thunk": "2.2.0",
    "reselect": "3.0.1",
    "timeago.js": "3.0.2"
  },
  "devDependencies": {
    "angular-mocks": "1.6.9",
    "babel-core": "6.26.0",
    "babel-jest": "22.4.3",
    "babel-loader": "7.1.2",
    "babel-plugin-angularjs-annotate": "0.8.2",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-preset-env": "1.6.1",
    "babel-preset-react": "6.24.1",
    "clean-webpack-plugin": "0.1.18",
    "css-loader": "0.28.7",
    "cssnano": "3.10.0",
    "eslint": "4.18.2",
    "eslint-plugin-jest": "21.15.1",
    "eslint-plugin-react": "7.8.2",
    "extract-text-webpack-plugin": "3.0.1",
    "favicons-webpack-plugin": "0.0.7",
    "file-loader": "1.1.5",
    "html-loader-jest": "0.2.1",
    "html-webpack-plugin": "2.30.1",
    "intersection-observer": "0.5.0",
    "jest": "22.4.3",
    "jest-css-modules-transform": "1.1.0",
    "jest-fetch-mock": "1.5.0",
    "jest-sonar-reporter": "2.0.0",
    "lodash.ismatch": "4.4.0",
    "mock-local-storage": "1.0.5",
    "null-loader": "0.1.1",
    "optimize-css-assets-webpack-plugin": "3.2.0",
    "raw-loader": "0.5.1",
    "react-test-renderer": "16.4.1",
    "redux-mock-store": "1.3.0",
    "snyk": "1.71.0",
    "style-loader": "0.19.0",
    "stylelint": "9.2.1",
    "stylelint-config-standard": "18.2.0",
    "svg-url-loader": "2.2.1",
    "webpack": "3.8.1",
    "webpack-bundle-analyzer": "2.9.0",
    "webpack-dev-server": "2.9.3"
  },
  "directories": {
    "test": "test"
  },
  "jestSonar": {
    "reportPath": "build",
    "reportFile": "TESTS.xml"
  }
}

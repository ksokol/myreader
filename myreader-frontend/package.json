{
  "name": "myreader-frontend",
  "description": "Myreader frontend package",
  "scripts": {
    "build": "export NODE_ENV=production && rimraf dist && webpack --bail --progress --profile --display-optimization-bailout",
    "server": "export NODE_ENV=development && webpack-dev-server --history-api-fallback --inline --progress",
    "test": "export NODE_ENV=development && karma start",
    "test-watch": "export NODE_ENV=development && npm test -- --single-run=false --reporters=progress,coverage-istanbul",
    "start": "npm run server",
    "report": "export NODE_ENV=production && rimraf dist && webpack --bail --progress --profile && webpack-bundle-analyzer dist/stats.json"
  },
  "dependencies": {
    "angular": "1.5.8",
    "angular-animate": "1.5.8",
    "angular-aria": "1.5.8",
    "angular-hotkeys": "1.5.0",
    "angular-material": "1.1.4",
    "angular-messages": "1.5.8",
    "angular-sanitize": "1.5.8",
    "angular-ui-router": "0.2.15",
    "timeago.js": "3.0.1",
    "material-design-icons": "3.0.1"
  },
  "devDependencies": {
    "angular-mocks": "1.5.8",
    "babel-core": "6.26.0",
    "babel-loader": "7.1.2",
    "babel-plugin-angularjs-annotate": "0.8.2",
    "babel-preset-env": "1.6.1",
    "css-loader": "0.28.7",
    "cssnano": "3.10.0",
    "extract-text-webpack-plugin": "3.0.1",
    "favicons-webpack-plugin": "0.0.7",
    "file-loader": "1.1.5",
    "html-webpack-plugin": "2.30.1",
    "istanbul-instrumenter-loader": "3.0.0",
    "jasmine-core": "2.8.0",
    "karma": "1.7.1",
    "karma-chrome-launcher": "2.2.0",
    "karma-coverage-istanbul-reporter": "1.3.0",
    "karma-jasmine": "1.1.0",
    "karma-sonarqube-unit-reporter": "0.0.13",
    "karma-sourcemap-loader": "0.3.7",
    "karma-webpack": "2.0.5",
    "node-libs-browser": "2.0.0",
    "null-loader": "0.1.1",
    "optimize-css-assets-webpack-plugin": "3.2.0",
    "raw-loader": "0.5.1",
    "rimraf": "2.6.2",
    "style-loader": "0.19.0",
    "svg-url-loader": "2.2.1",
    "webpack": "3.8.1",
    "webpack-bundle-analyzer": "2.9.0",
    "webpack-dev-server": "2.9.3"
  },
  "directories": {
    "test": "test"
  }
}
